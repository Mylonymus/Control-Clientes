field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"4\">\n                    <v-text-field\n                      label=\"Teléfono2*\"\n                      v-model=\"cliente.telefono2\"\n                      type=\"text\"\n                      hint=\"Teléfono Contacto 2\"\n                      required\n                    ></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"4\">\n                    <v-text-field\n                      label=\"Nombre Contacto*\"\n                      v-model=\"cliente.nombreContacto\"\n                      type=\"text\"\n                      hint=\"Nombre Contacto\"\n                      required\n                    ></v-text-field>\n                  </v-col>\n\n                  <v-col cols=\"12\" sm=\"4\">\n                    <v-select\n                      v-model=\"cliente.status\"\n                      :items=\"['EN PROGRESO', 'ACTIVO', 'INACTIVO', 'STAND-BY']\"\n                      label=\"Status*\"\n                      required\n                    ></v-select>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\">\n                    <v-text-field\n                      label=\"Página Web*\"\n                      v-model=\"cliente.url\"\n                      type=\"text\"\n                      hint=\"Dirección Web\"\n                      required\n                    ></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\">\n                    <v-row>\n                      <v-col cols=\"12\" sm=\"4\">\n                        <v-switch v-model=\"cliente.web\" label=\"Web\"></v-switch>\n                      </v-col>\n                      <v-col cols=\"12\" sm=\"4\">\n                        <v-switch v-model=\"cliente.fba\" label=\"Facebook Ads\"></v-switch>\n                      </v-col>\n                      <v-col cols=\"12\" sm=\"4\">\n                        <v-switch v-model=\"cliente.ga\" label=\"Google Ads\"></v-switch>\n                      </v-col>\n                    </v-row>\n                    <v-row>\n                      <v-col cols=\"12\" sm=\"4\">\n                        <v-switch v-model=\"cliente.fb\" label=\"Facebook\"></v-switch>\n                      </v-col>\n                      <v-col cols=\"12\" sm=\"4\">\n                        <v-switch v-model=\"cliente.instagram\" label=\"Instagram\"></v-switch>\n                      </v-col>\n                      <v-col cols=\"12\" sm=\"4\">\n                        <v-switch v-model=\"cliente.twitter\" label=\"Twitter\"></v-switch>\n                      </v-col>\n                    </v-row>\n                  </v-col>\n\n                  <v-col cols=\"12\" sm=\"4\">\n                    <v-select\n                      v-model=\"cliente.plan\"\n                      :items=\"['Starter', 'Business', 'Premium', 'Social', 'Enterprise']\"\n                      label=\"Plan*\"\n                      required\n                    ></v-select>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\">\n                    <v-text-field label=\"URL Reporte Mensual*\" type=\"text\" required></v-text-field>\n                  </v-col>\n                </v-row>\n              </v-container>\n              <small>*indicates required field</small>\n            </v-card-text>\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Close</v-btn>\n              <v-btn color=\"blue darken-1\" text @click=\"onSubmit\">Save</v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n      </v-row>\n    </template>\n  </div>\n</template>\n \n\n<script>\nimport { db } from \"../firebase\";\n\nexport default {\n\n  firebase:{\n    clientes: db.ref('clientes')\n  },\n  data: () => ({\n    dialog: false,\n    cliente: {\n      nombre: \"\",\n      rs: \"\",\n   